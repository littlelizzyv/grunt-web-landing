
{{#each elways.artist-info}}
<div class="content-block">
	<h1 class="bandTitle"><span class="bandDate">{{ bandDate }}</span><span class="bandName">{{ bandName }}</span></h1>
	<div class="loading">
			<i class="fa fa-spinner fa-spin"></i>
	</div>
	<div class="soundcloud-widget">
		<iframe class="tntSC" width="100%" height="166" scrolling="no" frameborder="no" src="{{ soundSrc }}&amp;color=ff5500&amp;auto_play=false&amp;hide_related=false&amp;show_comments=true&amp;show_user=true&amp;show_reposts=false" defer ></iframe>
	</div> 
	<div class="rsvp-button">
		<div class="button-img">
		<a href="{{ rsvpUrl }}" target="_blank"><img src="https://s3.amazonaws.com/elways/taste%26tunes/rsvpFacebook.png"></a>
		</div>
	</div>
	<hr class="band-hr">
</div>
{{/each}}


<div class="sponsor-block">
	<div class="sponsor">
		<div class="sponsor-component">
			<h4>Sponsored By</h4>
		</div>
		<div class="sponsor-component">
			{{#each elways.sponsors}}
			<div class="sponsor-logo">
				<a href="{{ sponsor-url }}"><img src="{{ sponsor-img }}"></a>
			</div>
			{{/each}}
		</div>
	</div> 
</div>

